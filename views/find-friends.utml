<h1>Choose friends to follow</h1>

<ul class="thumbnails">

  <% _.each(found, function(user) { %>

  <li class="span3">
    <div class="thumbnail">
      <img src="<%- user.avatar %>" width="96" height="96" alt="<%- user.name %>">
      <h3><%- user.name %> <small><%- user.id %></small></h3>
    </div>
  </li>

  <% }); %>

</ul>
